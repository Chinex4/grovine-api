        {
          "name": "Create Recipe (multi-step final submit)",
          "request": {
            "method": "POST",
            "body": {
              "mode": "formdata",
              "formdata": [
                { "key": "video", "type": "file", "src": "" },
                { "key": "cover_image", "type": "file", "src": "" },
                { "key": "title", "value": "Egusi Soup With Chicken", "type": "text" },
                { "key": "short_description", "value": "Short preview description", "type": "text" },
                { "key": "instructions", "value": "Step 1...\\nStep 2...\\nStep 3...", "type": "text" },
                { "key": "duration_seconds", "value": "900", "type": "text" },
                { "key": "servings", "value": "2", "type": "text" },
                { "key": "estimated_cost", "value": "20000", "type": "text" },
                { "key": "is_quick_recipe", "value": "false", "type": "text" },
                { "key": "ingredients[0][item_text]", "value": "2 red onions, diced", "type": "text" },
                { "key": "ingredients[0][product_id]", "value": "{{product_id}}", "type": "text" },
                { "key": "ingredients[0][cart_quantity]", "value": "2", "type": "text" },
